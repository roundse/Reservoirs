function m = addInternalConn(m,w,q)
            % Update degree counts.
        temp = w_exc_exc{1};
        temp(temp>0) = 1;
        D_exc = sum(temp);
        totalDExc = sum(D_exc);
      
        % Get probability
        P_exc = cumsum(D_exc ./ totalDExc);
        r1 = rand;
        inx_exc1 = find([-1 P_exc] < r1, 1, 'last');
        r2 = rand;
        inx_exc2 = find([-1 P_exc] < r2, 1, 'last');
        w_exc_exc{1}(inx_exc1,inx_exc2) = excWght;
end